<odoo>
    <data>
        <record id="view_nivel_form" model="ir.ui.view">
            <field name="name">academico nivel form</field>
            <field name="model">academico.nivel</field>
            <field name="arch" type="xml">
                <form string="nivel">
                    <sheet>
                        <group>
                            <field name="name"/> 
                            
                        </group>
                    </sheet>
                </form>
            </field>
          </record>
          
          <record id="view_nivel_tree" model="ir.ui.view">
            <field name="name">academico nivel list</field>
            <field name="model">academico.nivel</field>
            <field name="arch" type="xml">
                <tree string="nivel">
                    <field name="name"/>
              
                    
                </tree>
            </field>
          </record>
    
          <record model="ir.actions.act_window" id="academico.action_nivel_window">
            <field name="name">academico nivel window</field>
            <field name="res_model">academico.nivel</field>
            <field name="view_mode">tree,form</field>
          </record>
          
          <menuitem name="Nivel" id="academico.menu_1_nivel_list" parent="academico.menu_1"
          action="academico.action_nivel_window"/>
    </data>
</odoo>